<%= simple_form_for(@concert) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :startdate %>
    <%= f.input :enddate %>
    <%= f.input :poster %>
    <%= f.input :reflink %>
    <%= f.input :description %>
    <%= f.input :status %>
    <%= f.input :category %>
    <%= f.input :score %>
    <%= f.input :breakday %>
    <%= f.input :price %>
	<%= f.input :location %>
    <%= f.input :available %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
